{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
  "name": "evntdrvn",
  "compatibility_date": "2026-01-01",
  "assets": {
    "directory": "./public"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": true,
      "persist": true,
      "head_sampling_rate": 1
    }
  }
}
